<h1 class='title'><%= blog.get('name') %></h1>

<hr>
<div class='body'><%= blog.get('description') %></div>

<br><br>
<hr>

<h2>Posts</h2>
<ul class="posts">
  <% blogPosts.each(function(post) { %>
    <li><%= post.escape('title') %></li>
  <% }); %>
</ul>
<br>
<input type='text' class='newPostTitle'>
<textarea class='newPostBody' rows='6' cols='39'></textarea>
<button class='addPostBtn'>Add Post</button>